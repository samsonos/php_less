{
  "name": "samsonphp/less",
  "author": "Vitaly Iegorov <egorov@samsonos.com>",
  "minimum-stability": "dev",
  "require": {
    "samsonphp/event": "1.*",
    "samsonos/php_core": "*",
    "samsonos/php_resourcer": "*",
    "leafo/lessphp": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "4.*"
  },
  "replace": {
    "samsonos/php_less":"*"
  },
  "license": "Open Software License (OSL) v 3.0",
  "homepage": "http://samsonos.com/",
  "config": {
    "github-protocols": [
      "https"
    ]
  },
  "autoload": {
    "classmap": [
      "src/"
    ],
    "psr-4": {
      "samsonphp\\less\\": "src/"
    }
  },
  "extra": {
    "samson_module_include": "1"
  }
}
